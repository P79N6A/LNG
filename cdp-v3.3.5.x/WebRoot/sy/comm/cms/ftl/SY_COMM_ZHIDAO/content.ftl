					<div class="panel-container">
						<div class="question-invite-panel" style="display: none;"></div>
						<div id="comment-${ask.Q_ID}" class="zm-comment-box" data-count="2"></div>
						<!-- 加载评论 -->
						<script type="text/javascript">
							var cmtBtn = jQuery("#ask-${ask.Q_ID}");
							var commentDiv = jQuery("#comment-${ask.Q_ID}");
							commentDiv.attr("style","display: none;"); 
							var opts = {
									"COMMENT_SERV" : "SY_COMM_ZHIDAO_QUESTION_COMMENT"
									"DATA_ID" : "${ask.Q_ID}",
									"SERV_ID" : "SY_COMM_ZHIDAO_QUESTION",
									"SHOWNUM" : 10,
									"NOWPAGE" : 1,
									"pCon" : commentDiv
								};
							var listView = new rh.vi.zhidao_comment(opts);
							cmtBtn.unbind("click").bind("click", function() {
								if (false == listView.isShow()) {
									commentDiv.attr("style","display: block;"); 
								    //布局初始化
								    listView.layout();
								    //展示数据
									listView.show();
								} else {
									commentDiv.attr("style","display: none;"); 
									listView.remove();
								}
							});
						</script>




					</div>

				</div>
